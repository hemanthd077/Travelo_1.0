                                            <div class="inner-feedback-div">
                                                <div class="ratingstar-block-div">
                                                    <p class="feedback-txt">What you think of this tour</p>
                                                    <div class="rating-star-div" style="margin-top: 30px;">
                                                        {{#each this.[15]}}
                                                            {{#if this}}
                                                                <button id="ratingbutton" type="button" onclick="toggleStar('{{@index}}','{{outerIndex}}')"><i class="fa-solid fa-star fa-3x rating-star ratingstat1-{{outerIndex}}{{@index}}"></i></button>   
                                                            {{else}}
                                                                <button id="ratingbutton" type="button" onclick="toggleStar('{{@index}}','{{outerIndex}}')"><i class="fa-regular fa-star fa-3x rating-star ratingstat1-{{outerIndex}}{{@index}}"></i></button>
                                                            {{/if}}
                                                        {{/each}}
                                                    </div>
                                                </div>
                                                <div class="ratingstar-block-div-1">
                                                    <p class="feedback-txt">Do You have any thoughts you'd like to share?</p>
                                                    <div class="inner-ratingstar-block-div">
                                                        <textarea name="feedback" id="textarea-feedback" cols="90" rows="8" placeholder="Tell us in words..." required></textarea>
                                                        <div class="ratingstar-block-div-button">
                                                            <button class="submit-feedback" type="submit" onclick="dragupfeedback('{{@index}}')">Submit Feedback</button>
                                                            <button class="close-feedback" onclick="dragupfeedback('{{@index}}')">Close</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    {{/each}}
                            {{/if}} 
                        </div>
                    {{/if}}
