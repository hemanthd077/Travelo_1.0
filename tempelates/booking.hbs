                                                                {{/each}}
                                                            </div>
                                                        </div>
                                                        <div>
                                                            <button id="givefeedback-btn" onclick="dragdownfeedback('{{@index}}')">Give Feedback here <br> <i class="fa-solid fa-truck-arrow-right"></i></button>
                                                        </div>
                                                    </div>
