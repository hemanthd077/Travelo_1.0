                                    <div class="svgfiller"></div> --}}
                                {{else}}
                                    {{#each completedTour as |outerItem outerIndex|}}
                                        <div id="inner-content-block">
                                            <div>
                                                {{#if this.[12]}}
                                                    <div class="statusheadercolor-complete"> </div>
                                                {{/if}}
                                            </div>
                                            <div id="inner-buscontent">
                                                <img id="busimage" src="data:{{this.[3]}}" alt="">
                                                <div style="display: flex;flex-direction:column;">
                                                    <div class="buscontent-main-div">
