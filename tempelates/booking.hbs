<script>
    try{
        function toggleStar(innerindex,outerIndex){
            for(let i=0;i<5;i++){
                let togglestarrating = document.querySelector(`.ratingstat-${outerIndex}${i}`);
                if(i<=innerindex){
                    if(togglestarrating.classList.contains('fa-regular')){
                        togglestarrating.classList.remove('fa-regular');
                        togglestarrating.classList.add('fa-solid'); 
                    }  
                }
                else{
                    if(togglestarrating.classList.contains('fa-solid')){
                        togglestarrating.classList.remove('fa-solid');
                        togglestarrating.classList.add('fa-regular'); 
                    }  
                }

                togglestarrating = document.querySelector(`.ratingstat1-${outerIndex}${i}`);
                if(i<=innerindex){
                    if(togglestarrating.classList.contains('fa-regular')){
                        togglestarrating.classList.remove('fa-regular');
                        togglestarrating.classList.add('fa-solid'); 
                    }  
                }
                else{
                    if(togglestarrating.classList.contains('fa-solid')){
                        togglestarrating.classList.remove('fa-solid');
                        togglestarrating.classList.add('fa-regular'); 
                    }  
                }

                const paragraph = document.querySelector('.ratingstatus');
                if(innerindex==='4'){
                    paragraph.textContent = 'Absolutely Excellent';
                }
                else if(innerindex==='3'){
                paragraph.textContent = 'Very Satisfied!';
                }
                else if(innerindex==='2'){
                    paragraph.textContent = 'Average Experience';
                }
                else if(innerindex==='1'){
                    paragraph.textContent = 'Could be better';
                }
                else{
                    paragraph.textContent = 'Not satisfied at all';
                }

            }
            let response=  fetch(`/ratingUpdate?points=${innerindex}&index=${outerIndex}`, {
                method: 'POST',
            });
        }
    }
    catch(e){
        console.log(e);
    }
    try{
        document.getElementById('dealerimage').setAttribute('draggable', false);
        document.getElementById('busimage').setAttribute('draggable', false);
        document.getElementById('appicon').setAttribute('draggable', false);
    }
    catch(e){
        console.log(e);
    }
    function activestatus(id1,id2,id3){
        const element1 = document.getElementById(`bookinnerheader${id1}`);
        if(element1){
            element1.classList.remove(`bookinnerheader${id1}`);
            element1.classList.add('bookinnerheaderactive');
        }

        const element2 = document.getElementById("bookinnerheader"+id2);
        if(element2){
            element2.classList.remove(`bookinnerheaderactive`);
            element2.classList.add(`bookinnerheader${id2}`);
        }

        const element3 = document.getElementById("bookinnerheader"+id3);
        if(element3){
            element3.classList.remove(`bookinnerheaderactive`);
            element3.classList.add(`bookinnerheader${id3}`);
        }

        const innercontentblock1 = document.getElementById(`booking-content-div-${id1}`)
        if(innercontentblock1){
            innercontentblock1.style.display='flex';
        }
        const innercontentblock2 = document.getElementById(`booking-content-div-${id2}`)
        if(innercontentblock2){
            innercontentblock2.style.display='none';
        }

        const innercontentblock3 = document.getElementById(`booking-content-div-${id3}`)
        if(innercontentblock3){
            innercontentblock3.style.display='none';
        }

        setTimeout(function(){
            document.querySelector(`.booking-loading-${id1}`).style.display = "none";
            document.querySelector(`.booking-content-${id1}`).style.display = "flex";
        },2000);
    }

    activestatus('1','2','3');

    try{
        function dragdownfeedback(index){
            document.querySelector(`.drag-feedback-${index}`).style.marginTop = "-20px";
            setTimeout(function(){
                document.querySelector(`.drag-feedback-${index}`).style.zIndex = "0";
            },800);
            document.querySelector(`.drag-feedback-${index}`).style.boxShadow = '0px 3px 5px -2px #2d3467';
        }
    }
    catch(e){
        console.log(e);
    }

    try{
        function dragupfeedback(index){
            document.querySelector(`.drag-feedback-${index}`).style.zIndex = "-1";
            setTimeout(function(){
                document.querySelector(`.drag-feedback-${index}`).style.marginTop = "-250px";
            },500);
        }
    }
    catch(e){
        console.log(e);
    }
</script>

</html>
    
