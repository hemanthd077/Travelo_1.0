                                                                {{#each this.[15]}}
                                                                    {{#if this}}
                                                                        <button id="ratingbutton" type="button" onclick="toggleStar('{{@index}}','{{outerIndex}}')"><i class="fa-solid fa-star fa-2x rating-star ratingstat-{{outerIndex}}{{@index}}"></i></button>   
                                                                    {{else}}
