                                                                <p class="outer-managerheader-title"><i class="fa-solid fa-user-tie"></i> Guide Assigned</p>
                                                                <div class="manager-inner-class">
                                                                    <div class="inner-managercontent-namediv">
                                                                        <p class="managerheader-title">Name</p>
                                                                        <p><i class="fa-solid fa-seedling fa-xs"></i> {{this.[16].[0]}}</p>
                                                                    </div>
                                                                    <div class="inner-managercontent-div">
                                                                        <p class="managerheader-title">Contact No</p>
                                                                        <a href="tel:{{this.[16].[1]}}" class="contactnumber-class"><p><i class="fa-solid fa-phone fa-xs"></i> {{this.[16].[1]}}</p></a>
                                                                    </div>
                                                                    <div class="inner-managercontent-div">
                                                                        <p class="managerheader-title">Gender</p>
                                                                        <p><i class="fa-solid fa-venus-mars fa-xs"></i> {{this.[16].[2]}}</p>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                    {{else}}
